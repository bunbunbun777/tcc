@INPROCEEDINGS{Hutchings2007,
  author = {Hutchings, Tim and Jeffryes, Susan},
  title = {Architecting UAV sense \& avoid systems},
  booktitle = {Autonomous Systems, 2007 Institution of Engineering and Technology
	Conference on},
  year = {2007},
  pages = {1--8},
  abstract = {For Unmanned Aerial Vehicles (UAVs) to operate in civil airspace alongside
	manned aviation, they must prove that they can operate to an Equivalent
	Level of Safety (ELOS) to that of manned aviation. Commercial use
	of UAVs is currently limited by their inability to detect, sense
	and avoid airborne hazards. This paper discusses the meaning of ELOS,
	and the system requirements that it creates. It also discusses Thales
	UK Ltd (Aerospace division) progress on Sense \&\#x00026; Avoid (S\&\#x00026;A)
	system and subsystem design to meet these requirements as part of
	the Autonomous Systems Technology Related Airborne Evaluation \&\#x00026;
	Assessment (ASTRAEA) commercial UAV programme.},
