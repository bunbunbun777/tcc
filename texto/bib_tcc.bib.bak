@ARTICLE{Akyildiz2007,
  author = {Akyildiz, I. and Melodia, T. and Chowdhury, K.},
  title = {A survey on wireless multimedia sensor networks},
  journal = {Computer Networks},
  year = {2007},
  volume = {51},
  pages = {921--960},
  number = {4},
  month = {March},
  abstract = {The availability of low-cost hardware such as CMOS cameras and microphones
	has fostered the development of Wireless Multimedia Sensor Networks
	(WMSNs), i.e., networks of wirelessly interconnected devices that
	are able to ubiquitously retrieve multimedia content such as video
	and audio streams, still images, and scalar sensor data from the
	environment. In this paper, the state of the art in algorithms, protocols,
	and hardware for wireless multimedia sensor networks is surveyed,
	and open research issues are discussed in detail. Architectures for
	WMSNs are explored, along with their advantages and drawbacks. Currently
	off-the-shelf hardware as well as available research prototypes for
	WMSNs are listed and classified. Existing solutions and open research
	issues at the application, transport, network, link, and physical
	layers of the communication protocol stack are investigated, along
	with possible cross-layer synergies and optimizations.},
  citeulike-article-id = {1036979},
  citeulike-linkout-0 = {http://dx.doi.org/10.1016/j.comnet.2006.10.002},
  citeulike-linkout-1 = {http://linkinghub.elsevier.com/retrieve/pii/S1389128606002751},
  citeulike-linkout-2 = {http://www.sciencedirect.com/science/article/B6VRG-4M813XK-2/2/8fd2c2ae239c3065d3130e053e467ba1},
  day = {14},
  doi = {10.1016/j.comnet.2006.10.002},
  issn = {13891286},
  keywords = {multimedia, networks, sensor, wireless},
  owner = {Netto},
  posted-at = {2008-12-30 15:35:01},
  priority = {2},
  timestamp = {2010.05.27},
  url = {http://dx.doi.org/10.1016/j.comnet.2006.10.002}
}

